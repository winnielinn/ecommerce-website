<h1>hello</h1>
{{! action = API 網址 }}
<form name="newebPay" action="{{tradeInfo.PayGateWay}}" method="POST">

  {{! 明碼參數 MID = MerchantID }}
  <input
    type="text"
    name="MerchantID"
    value="{{tradeInfo.MerchantID}}"
    hidden
  />

  {{! 加密後參數 TradeInfo }}
  <input type="text" name="TradeInfo" value="{{tradeInfo.TradeInfo}}" hidden />

  {{! 壓碼參數 TradeSha }}
  <input type="text" name="TradeSha" value="{{tradeInfo.TradeSha}}" hidden />

  {{! 版本 Version}}
  <input type="text" name="Version" value="{{tradeInfo.Version}}" hidden />

</form>